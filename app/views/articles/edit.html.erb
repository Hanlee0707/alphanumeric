<% provide(:title, "Edit Article") %>

<% if @isEditor then %>
<%= render partial: 'articles/editor/editor_form', locals: { isEditor: true, editor: @editor_id } %>
<% elsif @isContributor then %>
<%= render partial: 'articles/contributor/contributor_form', locals: { isContributor: true, contributor: @contributor_id, initiated: @contributorInit } %>
<% end %>








