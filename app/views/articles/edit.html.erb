<% provide(:title, "Edit Article") %>

<% if @isEditor then %>
<%= render partial: 'editor_form', locals: { isEditor: true, editor: @editor_id } %>
<% elsif @isContributor then %>
<%= render partial: 'contributor_form', locals: { isContributor: true, contributor: @contributor_id, initiated: @contributorInit } %>
<% end %>








