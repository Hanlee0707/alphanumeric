jQuery.noConflict();
jQuery(document).ready(function($) { 
  var hidden_element = $('#searched_contributor_id');
  hidden_element.val("<%= @employee.id %>");
  $("#contributor_id").val(hidden_element.val());	
  $("#contributor_query").hide();
  $("#contributor_container").show();
  $("#contributor_container").html("<%= @employee.full_name_with_email %>");
  $("#remove_contributor_button_container").show();
});

